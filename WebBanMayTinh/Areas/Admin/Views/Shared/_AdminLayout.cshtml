@{
    var user = Session["Email"];
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - TTG Shop</title>
    <link rel="stylesheet" href="~/Content/CSS/CustomerLayout.css" />

    <link rel="stylesheet" href="~/Content/Icon/themify-icons-font/themify-icons/themify-icons.css">

    @*Font-awesome*@
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")


    <style>
        .collapse .navbar-nav > li {
            margin: 0;
        }
    </style>
</head>



<body>
    <header>

        @Html.Partial("PV_Header_Admin_Layout");

    </header>



    <div class="container body-content" style="height:1000px">
        @RenderBody()
    </div>


    <footer>

        @Html.Partial("PV_Footer_Admin_Layout");

    </footer>
</body>
</html>



@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrap")
@RenderSection("scripts", required: false)